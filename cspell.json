{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "es",
  "dictionaries": ["spanish", "es-es", "html", "css", "technical"],
  "useGitignore": true,
  "ignorePaths": [
    "**/vendor/**",
    "**/node_modules/**",
    "**/storage/**",
    "**/public/**",
    "**/bootstrap/**",
    "**/build/**",
    "cspell.json",
    ".vscode/**"
  ],
  "ignoreRegExpList": [
    "/class=\"[^\"]*\"/g",
    "/class='[^']*'/g",
    "/<[^>]+>/g",
    "/@\\w+/g",
    "/:\\w+/g"
  ],
  "languageSettings": [
    {
      "languageId": "html",
      "dictionaries": ["spanish", "es-es"],
      "ignoreRegExpList": ["@\\w+", "<[^>]+>"]
    },
    {
      "languageId": "php",
      "dictionaries": ["spanish", "es-es"],
      "ignoreRegExpList": ["@\\w+", "<[^>]+>"]
    },
    {
      "languageId": "blade",
      "dictionaries": ["spanish", "es-es"],
      "ignoreRegExpList": ["class=\"[^\"]*\"", "<[^>]+>", "@\\w+"]
    }
  ],
